{
  "author": {
    "email": "anthony.williams.cs@gmail.com",
    "name": "Anthony Williams",
    "url": "https://github.com/Wolven531/angular-game.git"
  },
  "name": "angular-game",
  "private": true,
  "pre-commit": [
    "version:bump",
    "version:addPackage"
  ],
  "scripts": {
    "build": "ng build",
    "build:ci": "npm run clean && npm run test && npm run build:prod",
    "build:prod": "ng build --prod",
    "clean": "rimraf ./dist",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "ng": "ng",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "start": "ng serve",
    "start:open": "ng serve --open",
    "test": "ng test --watch=false --browsers=ChromeHeadlessCustom",
    "test:coverage": "ng test --no-watch --code-coverage --browsers=ChromeHeadlessCustom",
    "test:watch": "ng test",
    "version:addPackage": "git add ./package.json",
    "version:bump": "node ./bumpVersion.js patch"
  },
  "version": "0.67.35",
  "dependencies": {
    "@angular/animations": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "tslib": "^1.11.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.8",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "codelyzer": "^5.2.2",
    "eslint": "^6.8.0",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "pre-commit": "^1.2.2",
    "protractor": "^5.4.3",
    "puppeteer": "^2.1.1",
    "ts-node": "^8.8.1",
    "tslint": "~5.20.1",
    "typescript": "~3.7.5"
  }
}
